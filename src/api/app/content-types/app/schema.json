{
  "kind": "collectionType",
  "collectionName": "apps",
  "info": {
    "singularName": "app",
    "pluralName": "apps",
    "displayName": "app",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "guild": {
      "type": "richtext"
    },
    "source_url": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "related_apps": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::app.app",
      "inversedBy": "apps"
    },
    "apps": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::app.app",
      "mappedBy": "related_apps"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "app_id"
    }
  }
}
